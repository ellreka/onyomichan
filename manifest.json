{
  "manifest_version": 2,
  "name": "on読みちゃん",
  "description": "おーぷん２ちゃんねるのスレッドを読み上げる拡張機能",
  "version": "1.0.0",

  "browser_action": {
    "default_popup": "popup.html"
  },



  "web_accessible_resources": [
    "button.html"
  ],
"options_ui":{
  "page":"options.html",
   "chrome_style":true
},

"content_scripts": [
    {
        "matches":  ["http://*.open2ch.net/test/*","http://*.open2ch.net//test/*"],
        "js": ["jquery-3.2.1.min.js","contentscript.js"],
        "css":["style.css"]
    }
],




//  "icons":{
//    "16": ".png",
//     "48": ".png",
//    "128": ".png"
//  },



"permissions": [
  "tabs",
  "storage"
]


}

